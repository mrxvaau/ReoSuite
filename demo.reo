
# Demo Reo program
let name be "Joy".
say "Hello, " plus name plus "!".

to add(a, b):
    return a plus b.
end.

let nums be range(1, 5).
for each item in nums:
    say item.
end for each.

let total be add(40, 2).
say "Answer: " plus total.

repeat 3 times:
    say "tick".
end repeat.

let today be now().
say "Now: " plus today.
say format_now("yyyy-MM-dd HH:mm:ss").
